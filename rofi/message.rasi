@import "colors.rasi"

* {
    background-color:       @background;
    text-color:             @foreground;
	font:				 	"JetBrainsMono Nerd Font 11";
}

window {
    width:      			320px;
    padding:    			25px;
    border:					0px;
    border-radius:          0px;
    border-color:           @purple;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				@yellow;
}